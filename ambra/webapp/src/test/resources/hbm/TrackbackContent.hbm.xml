<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.topazproject.ambra.models" auto-import="false">

  <class name="TrackbackContent" table="TrackbackContent">
    <id name="id" type="string" column="trackbackContentUri" length="255">
      <generator class="org.ambraproject.hibernate.AmbraIdGenerator"/>
    </id>
    <property name="title" type="string" column="title"/>
    <property name="url" type="org.ambraproject.hibernate.URLType" column="url"/>
    <property name="blog_name" type="string" column="blog_name"/>
    <property name="excerpt" type="string" column="excerpt"/>
  </class>

</hibernate-mapping>